// standard 2D boards
.brown.is100 > .cg-board
  background-image url('../../images/board/svg/brown_100.svg')
.brown.is64 > .cg-board
  background-image url('../../images/board/svg/brown_64.svg')
.brown.orientation-white
  > .files > li-coord, fieldnumber
    &.coord-even
      color rgba(#b58863, 1)
    &.coord-odd
      color rgba(#f0d9b5, 1)
  > .ranks > li-coord
    &.coord-even
      color rgba(#f0d9b5, 1)
    &.coord-odd
      color rgba(#b58863, 1)
.brown.orientation-black
  > .files > li-coord
    &.coord-even
      color rgba(#f0d9b5, 1)
    &.coord-odd
      color rgba(#b58863, 1)
  > .ranks > li-coord, fieldnumber
    &.coord-even
      color rgba(#b58863, 1)
    &.coord-odd
      color rgba(#f0d9b5, 1)
