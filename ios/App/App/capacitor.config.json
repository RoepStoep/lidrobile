{
	"appId": "org.lidraughts.mobileapp",
	"appName": "lidraughts",
	"bundledWebRuntime": false,
	"webDir": "www",
	"backgroundColor": "000000ff",
	"appendUserAgent": "Lidrobile/7.0.5",
	"plugins": {
		"SplashScreen": {
			"androidSplashResourceName": "launch_splash",
			"launchAutoHide": false,
			"useDialog": false
		},
		"PushNotifications": {
			"presentationOptions": [
				"sound",
				"alert"
			]
		}
	}
}
